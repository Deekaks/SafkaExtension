{
    "name": "BetterWilma",
    "description": "null",
    "version": "1.1",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["https://turku.inschool.fi/*"],
        "js": ["src/content.js"],
        "css": ["content.css"]
      }
    ],
    "browser_action": {
      "default_title": "Add Text"
    },
    "manifest_version": 3,
    "web_accessible_resources": [{
      "resources": [
        "src/main.js",
        "src/showFoodAlert.js",
        "src/html/index.html",
        "index1.html"
      ],
      "matches": ["https://turku.inschool.fi/*"]
    }]
}